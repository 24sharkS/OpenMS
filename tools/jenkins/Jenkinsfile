pipeline {
    agent any

    stages {
        stage('Build') {
            parallel macos: {
                node('highsierra') {
                    echo 'build'
                }
            },
            windows: {
                node('win10') {
                    echo 'build'
                }
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
